###############################################################################
#
# COMMANDS.CFG - Generated by Chef - local changes will be overwritten
#
# This config file provides some command definitions that one can reference
# in host, service, and contact definitions.
#
###############################################################################

<% @commands.each do |command| -%>

define command {
	<% command.each do |k,v| -%>
	<%= "#{k}	#{v}" %>
	<% end %>
}

<% end -%>
