###############################################################################
#
# TIMEPERIODS.CFG - Generated by Chef - local changes will be overwritten
#
# This config file provides some timeperiod definitions that one can reference
# in host, service, contact, and dependency definitions.
#
###############################################################################

<% @timeperiods.each do |timeperiod| -%>

define timeperiod {
	<% timeperiod.each do |k,v| -%>
	<%= "#{k}	#{v}" %>
	<% end %>
}

<% end -%>
