###############################################################################
#
# SERVICES.CFG - Generated by Chef - local changes will be overwritten
#
# This config file provides service definitions for all configured services.
#       
###############################################################################

<% @services.each do |service| -%>

define service {
	<% service.each do |k,v| -%>
	<%= "#{k}	#{v}" %>
	<% end %>
}

<% end -%>

<% @servicegroups.each do |servicegroup| -%>

define servicegroup {
	<% servicegroup.each do |k,v| -%>
	<%= "#{k}	#{v}" %>
	<% end %>
}

<% end -%>
